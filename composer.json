{
    "name": "darkroom/darkroom",
    "description": "A PHP library to transform, crop, resize and manipulate images",
    "keywords": [
        "gd",
        "image",
        "transform",
        "resize",
        "crop"
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "authors": [
        {
            "name": "Pablo Tejada",
            "email": "pablo@ptejada.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Darkroom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Darkroom\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
